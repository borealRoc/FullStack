{
    "presets": [
        [
            "@babel/preset-env",
            {
                // "useBuiltIns": "usage",
                "modules": false
                // "corejs": 2
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "absoluteRuntime": false,
                "corejs": false,
                "helpers": true,
                "regenerator": true,
                "useESModules": false,
                "version": "7.0.0-beta.0"
            }
        ]
    ]
}